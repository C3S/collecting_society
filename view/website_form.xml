<form col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="category"/>
    <field name="category"/>

    <group col="20" colspan="2" id="checkboxes">
        <label name="active"/>
        <field name="active" xexpand="0" width="25"/>
    </group>

    <label name="url"/>
    <field name="url"/>
    <label name="party"/>
    <field name="party"/>

    <notebook colspan="6">
        <page string="Resources" id="website_resources" col="1">
            <field name="website_resources"/>
        </page>
        <page string="Devices" id="devices" col="1">
            <field name="current_devices"/>
            <field name="device_assignments"/>
        </page>
    </notebook>
</form>
