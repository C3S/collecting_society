<form col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>

    <group col="20" colspan="2" id="checkboxes">
        <label name="active"/>
        <field name="active" xexpand="0" width="25"/>
    </group>

    <label name="description"/>
    <field name="description" colspan="5"/>

    <notebook colspan="6">
        <page string="Resources" id="website_resources" col="1">
            <field name="website_resources"/>
        </page>
        <page string="Website Categories" id="website_categories" col="1">
            <field name="website_categories"/>
        </page>
    </notebook>
</form>
