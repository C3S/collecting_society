<form string="Content" col="6">
    <label name="creation"/>
    <field name="creation"/>
    <label name="processing_state"/>
    <field name="processing_state"/>
    <label name="rejection_reason"/>
    <field name="rejection_reason"/>
    <newline/>
    <label name="mediation"/>
    <field name="mediation"/>
    <label name="processing_hostname"/>
    <field name="processing_hostname"/>
    <label name="duplicate_of"/>
    <field name="duplicate_of"/>
    <newline/>
    <label name="name"/>
    <field name="name"/>
    <label name="extension"/>
    <field name="extension"/>
    <label name="size"/>
    <field name="size"/>
    <newline/>
    <label name="category"/>
    <field name="category"/>
    <label name="mime_type"/>
    <field name="mime_type"/>
    <label name="length"/>
    <field name="length"/>

    <newline/>
    <label name="channels"/>
    <field name="channels"/>
    <label name="sample_rate"/>
    <field name="sample_rate"/>
    <label name="sample_width"/>
    <field name="sample_width"/>
    <label name="user"/>
    <field name="user"/>
    <label name="path"/>
    <field name="path" widget="url"/>
    <label name="preview_path"/>
    <field name="preview_path" widget="url"/>

    <label name="uuid"/>
    <field name="uuid"/>
    <label name="archive"/>
    <field name="archive"/>

    <notebook colspan="6">
        <page string="Duplicates" id="duplicates">
            <field name="duplicates" colspan="4"/>
        </page>
        <page string="Fingerprintlogs" id="fingerprintlogs">
            <field name="fingerprintlogs" colspan="4"/>
        </page>
    </notebook>
</form>
