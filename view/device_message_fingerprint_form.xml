<form string="Fingerprint" col="4">
    <label name="device"/>
    <field name="device"/>
    <label name="state"/>
    <field name="state"/>

    <label name="message"/>
    <field name="message" colspan="3"/>

    <newline/>

    <notebook colspan="4">
        <page string="Data" id="data" col="6">
            <label name="timestamp"/>
            <field name="timestamp"/>
            <label name="algorithm"/>
            <field name="algorithm"/>
            <label name="version"/>
            <field name="version"/>
            <field name="fingerprint" yexpand="1" colspan="6"/>
        </page>
        <page string="Processing" id="processing" col="2">
            <label name="matched_creation"/>
            <field name="matched_creation"/>
            <label name="merged_creation"/>
            <field name="merged_creation"/>
        </page>
    </notebook>
</form>