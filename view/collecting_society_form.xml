<form string="Collecting Society" col="4">
    <label name="name"/>
    <field name="name" colspan="2"/>
    <group col="20" colspan="1" id="checkboxes">
        <label name="active"/>
        <field name="active" xexpand="0" width="25"/>
    </group>
    <label name="party"/>
    <field name="party" colspan="3"/>
    <newline/>
    <group col="2" colspan="4" id="represents">
        <label name="represents_copyright"/>
        <field name="represents_copyright"/>
        <label name="represents_ancillary_copyright"/>
        <field name="represents_ancillary_copyright"/>
    </group>
</form>
