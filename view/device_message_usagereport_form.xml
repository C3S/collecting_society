<form string="Usage Report" col="6">
    <label name="device"/>
    <field name="device"/>
    <label name="message"/>
    <field name="message"/>
    <label name="state"/>
    <field name="state"/>

    <label name="timestamp"/>
    <field name="timestamp"/>
    <label name="creation"/>
    <field name="creation"/>

    <newline/>

    <notebook colspan="6">
        <page string="Indicators" id="indicators" col="2">
            <separator string="Reported" id="reported" colspan="2"/>
            <label name="reported_streams"/>
            <field name="reported_streams"/>
            <label name="reported_downloads"/>
            <field name="reported_downloads"/>
            <label name="reported_turnover_ads"/>
            <field name="reported_turnover_ads"/>
            <label name="reported_turnover_sale"/>
            <field name="reported_turnover_sale"/>
        </page>
        <page string="Processing" id="processing" col="2">
            <label name="utilisation_creation_list"/>
            <field name="utilisation_creation_list"/>
        </page>
    </notebook>
</form>